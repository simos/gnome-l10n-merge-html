msgstr ""

#: en/gimp-pimping.html:1378(td)
msgid "Zoom factor as a percentage"
msgstr ""

#: en/gimp-pimping.html:1381(td)
msgid "%s"
msgstr ""

#: en/gimp-pimping.html:1382(td)
msgid "Source scale factor (zoom level = %d/%s)"
