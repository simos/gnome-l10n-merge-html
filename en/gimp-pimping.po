#: en/gimp-pimping.html:1409(td)
msgid "%m"
msgstr ""

#: en/gimp-pimping.html:1410(td)
msgid "Memory used by the image"
msgstr ""

#: en/gimp-pimping.html:1413(td)
msgid "%n"
msgstr ""

#: en/gimp-pimping.html:1414(td)
msgid "Name of the active layer/channel"
msgstr ""

#: en/gimp-pimping.html:1417(td)
msgid "%P"
msgstr ""

