#: en/gimp-pimping.html:1385(td)
msgid "%d"
msgstr ""

#: en/gimp-pimping.html:1386(td)
msgid "Destination scale factor (zoom level = %d/%s)"
msgstr ""

#: en/gimp-pimping.html:1389(td)
msgid "%Dx"
msgstr ""

#: en/gimp-pimping.html:1390(td)
msgid "Expands to x if the image is dirty, nothing otherwise"
