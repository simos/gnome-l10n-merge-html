#: en/gimp-pimping.html:1367(td)
msgid "%i"
msgstr ""

#: en/gimp-pimping.html:1368(td)
msgid "View number, if an image has more than one display"
msgstr ""

#: en/gimp-pimping.html:1373(td)
msgid "%t"
msgstr ""

#: en/gimp-pimping.html:1374(td)
msgid "Image type (RGB, grayscale, indexed)"
msgstr ""

#: en/gimp-pimping.html:1377(td)
